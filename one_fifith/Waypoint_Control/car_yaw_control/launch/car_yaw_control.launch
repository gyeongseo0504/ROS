<launch>

	<node pkg="car_yaw_control" type="car_yaw_control_node" name="car_yaw_control_node" output="screen">
	
		<param name= "use_imu"                                type = "bool"                value = "true"/>
		<param name= "yaw_control_steering_output_topic"      type = "string"              value = "/Car_Control_Cmd/steering_control_mode"/>  
		<param name= "imu_yaw_angle_topic"                    type = "string"              value = "/imu/heading_angle_radian"/>		
		
		<param name= "xte_angle_control_topic"                type = "string"              value = "/Car_Control_Cmd/xte_steerAngle_Int16"/>
		<param name= "cross_track_error_topic"                type = "string"              value = "/virtual_cross_track_error"/> 
		
		<param name= "vision_cross_track_error_topic"         type = "string"              value = "/vision_xte"/>		
		
		<param name= "gps_heading_angle_topic"                type = "string"              value = "/gps_heading_angle"/>	
		<param name= "yaw_control_mode_topic"                 type = "string"              value = "/Car_Control_Cmd/steering_control_mode"/>   
		
		<param name= "yaw_target_topic"                       type = "string"              value = "/Car_Control_Cmd/Target_Angle"/>	    
		<param name= "traffic_cone_yaw_target_topic"          type = "string"              value = "/traffic_cone/heading_angle" />
	    
		<param name= "yaw_control_steering_output_topic"      type = "string"              value = "/Car_Control_Cmd/steerAngle_Int16"/> 
						
		<param name= "Kp"                                     type = "double"              value = "0.48"/>
		<param name= "Kd"                                     type = "double"              value = "47.5"/>
		<param name= "Ki"                                     type = "double"              value = "0.0"/>
		
		<param name= "Kp_xte"                                 type = "double"              value = "1.6"/>
		<param name= "Kd_xte"                                 type = "double"              value = "6.2"/>
		<param name= "Ki_xte"                                 type = "double"              value = "0.0"/>
		
		<param name= "Kp_vision"                              type = "double"              value = "0.06"/>
		<param name= "Kd_vision"                              type = "double"              value = "0.5"/>
		<param name= "Ki_vision"                              type = "double"              value = "0.0"/>
		
		<param name= "right_angle_max"                        type = "int"                 value = "-25"/>
		<param name= "left_angle_max"                         type = "int"                 value = " 25"/>
		
		<param name= "xte_right_angle_max"                    type = "int"                 value = "-6"/>
		<param name= "xte_left_angle_max"                     type = "int"                 value = " 6"/>
		
		<param name= "vision_xte_right_angle_max"             type = "int"                 value = "-15"/>
		<param name= "vision_xte_left_angle_max"              type = "int"                 value = " 15"/>
		
		
		
		
		
	</node>
</launch>
