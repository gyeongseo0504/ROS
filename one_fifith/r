alias use_ros2="unset ROS_DISTRO && unset AMENT_PREFIX_PATH && unset CMAKE_PREFIX_PATH && unset COLCON_PREFIX_PATH && unset PYTHONPATH && unset LD_LIBRARY_PATH && export PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin && source /opt/ros/humble/setup.bash && source ~/ros2_ws/install/setup.bash"


